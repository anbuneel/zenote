VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Sentry error monitoring (optional - leave empty to disable)
VITE_SENTRY_DSN=

# E2E Testing (optional - required for running Playwright E2E tests)
# Create a dedicated test user in Supabase for E2E testing
# These credentials should NEVER be committed - use .env.local (git-ignored)
E2E_TEST_EMAIL=
E2E_TEST_PASSWORD=
